//Write a program that takes a character as input and prints 1, 0, or -1 according to the following rules
#include<iostream>
using namespace std;

int main() {
	char ch;
    cin >> ch;
    if (ch>='A' && ch<='Z')
    {
        cout << 1 << endl;
    } else if (ch>='a' && ch<='z')
    {
        cout << 0 << endl;
    } 
    else {
        cout << -1 << endl;
    }
	
}
