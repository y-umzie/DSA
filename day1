//Write a program that takes a character as input and prints 1, 0, or -1 according to the following rules
#include<iostream>
using namespace std;

int main() {
	char ch;
    cin >> ch;
    if (ch>='A' && ch<='Z')
    {
        cout << 1 << endl;
    } else if (ch>='a' && ch<='z')
    {
        cout << 0 << endl;
    } 
    else {
        cout << -1 << endl;
    }
	
}

//Youâ€™re given a data type. Print its size in bytes
int dataTypes(string type) {
	if (type=="Integer")
	return 4;
	else if(type=="Long")
	return 8;
	else if(type=="Float")
	return 4;
	else if(type=="Double")
	return 8;
	else if(type=="Character")
	return 1;
}	


